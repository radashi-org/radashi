{
  "name": "radashi",
  "version": "12.1.0",
  "description": "Functional utility library - modern, simple, typed, powerful",
  "sideEffects": false,
  "author": "Alec Larson",
  "contributors": [
    "Ray Epps"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsup --clean --watch --sourcemap",
    "build": "tsup --clean",
    "lint": "tsc && biome check",
    "format": "biome check --fix --unsafe",
    "test": "vitest run --coverage",
    "prepublishOnly": "pnpm -s lint && pnpm -s build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@commitlint/config-conventional": "^19.2.2",
    "@vitest/coverage-v8": "^1.6.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.1.3",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/radashi-org/radashi"
  },
  "main": "dist/radashi.cjs",
  "module": "dist/radashi.js",
  "types": "dist/radashi.d.cts",
  "exports": {
    "require": {
      "types": "./dist/radashi.d.cts",
      "default": "./dist/radashi.cjs"
    },
    "types": "./dist/radashi.d.ts",
    "default": "./dist/radashi.js"
  }
}
